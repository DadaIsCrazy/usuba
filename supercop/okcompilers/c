clang -march=native -O3 -fomit-frame-pointer -fwrapv -std=gnu11
clang -mssse3 -O3 -fomit-frame-pointer -fwrapv -std=gnu11
icc -march=native -O3 -fomit-frame-pointer -fwrapv -std=gnu11
icc -xSSSE3 -O3 -fomit-frame-pointer -fwrapv -std=gnu11
icc -xSSE4.2 -O3 -fomit-frame-pointer -fwrapv -std=gnu11
icc -xSSE2 -O3 -fomit-frame-pointer -fwrapv -std=gnu11
